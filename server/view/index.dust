<!DOCTYPE html>
<html>

<head>
  <title>Hello React</title>
  <link rel="stylesheet" href="bower_components/bootstrap/dist/css/bootstrap.css" />
  <link rel="stylesheet" href="css/main.css">
  <link rel="stylesheet" href="css/consumer.css">
  <link rel="stylesheet" href="css/override_consumer.css">
  <link rel="stylesheet" href="css/new-default.css">
  <link rel="stylesheet" href="css/default.css">
  <link rel="stylesheet" href="css/ext-page.css">
</head>

<body>
  <div id="container">
    <!--START OF HEADER-->
    <div>
      <div class="fixed-header animate active">
        <div class="header-pane">
          <div class="logo">
            <a href="/"></a>
          </div>
          <div id="headerFormSearch" class="searchbox">
            <input name="searchTerm" type="text" class="blank" placeholder="Explore..." id="ui-search-text">
            <a href="" class="search-btn s-animate"></a>
          </div>
          <div class="site-navi">
            <div class="settings">
              <a href="javascript:void(0)" type="register">Join Veritone</a><a href="javascript:void(0)" type="login" style="margin-left:45px;">Log In</a>
            </div>
          </div>
        </div>
      </div>
    </div>
    <!--END OF HEADER-->
    <div class="">
      <div class="content-wrap ">
        <!-- ngIf: searchStart && relatedTopics.records -->
        <div class="blue-bar ">
          <div id="veritone-noti" class="confirm make-alert favorite-confirm">
            <span id="veritone-noti-text"></span>
            <a href="javascript:;;" class="ring_close" onclick="app.util.hideNotification();"></a>
          </div>
          <div class="contain">
            <!-- ngIf: relatedTopics.records -->
            <ul class="rel-search ">
              <!-- end ngRepeat: topic in trimmedRelatedTopics() -->
            </ul>
            <!-- end ngIf: relatedTopics.records -->
          </div>
        </div>
        <div class="contain min-height active">
          <div class="min-height ">
            <div class="exp-col1 ui-exp-coll min-height">
              <!-- ngInclude: filterTemplate -->
              <div class="page-title bb ">
                <div>
                  <!-- ngIf: hasSearchResult -->
                  <span id="resultCount" class="ui-has-search-result ">
                    <span>
                      <span class="f22 mr20">Clips</span>
                      <b>0</b>
                    </span>
                  </span>
                  <div id="advancedFilter" class="adv-holder ">
                    <a href="javascript:;;" class="adv-search animate ui-explore-filter">
                      Advanced Search
                    </a>
                    <div class="adv-dropdown sh-ani  hide">
                      <div class="filter-ftr">
                        <a href="javascript:;;" class="reset ui-clear-filter">Reset</a>
                        <a href="javascript:;;" class="primary ml20  s-animate small wide e2e-apply-button">Apply</a>
                      </div>
                    </div>

                  </div>
                  <!-- end ngIf: hasSearchResult -->
                </div>
              </div>
              <!-- ngIf: list.length > 0 -->
              <!-- ngInclude: mentionListTpl -->
              <div class="">
                <div class="">
                  <div class="offleft">
                  </div>
                  <div id="searchResult" class="unmask">
                  </div>
                </div>

              </div>
            </div>
            <div class="exp-col2 " id="ui-collection-search">
              <div class="page-title bb ">
                <a href="javascript:;;" ng-click="openAllRelatedCollections()" class="goright f13" style="padding-top:10px;" ng-show="true">View All Related Collections</a>
                <span class="f22 mr20">Collections</span>

              </div>
            </div>
            <!-- end ngIf: searchStart -->
          </div>
          <!--
          <script type="text/javascript" src="js/constant/appConstant.js"></script>
          <script type="text/javascript" src="js/constant/appConstant.js"></script>
          <script type="text/javascript" src="js/action/appAction.js"></script>
          <script type="text/javascript" src="js/store/appStore.js"></script>
          <script type="text/javascript" src="js/dispatcher/appDispatcher.js"></script>
          -->
          <!--<script type="text/javascript" src="js/react/react.min.js"></script>
          <script src="bower_components/jquery/dist/jquery.js"></script>-->
          <!--<script type="text/javascript" src="js/core.js"></script>
          <script type="text/javascript" src="js/component/search.js"></script>
          <script type="text/javascript" src="js/component/header.js"></script>-->
          <script type="text/javascript" src="js/searchPageView.js"></script>
          <!--<script type="text/javascript" src="js/searchPageHeader.js"></script>-->
</body>

</html>
