{
  "appenders": [{
    "type": "logLevelFilter",
    "level": "DEBUG",
    "appender": {
      "type": "console"
    }
  }, {
    "type": "logLevelFilter",
    "level": "INFO",
    "appender": {
      "type": "dateFile",
      "filename": "logs/veritone-connect.log",
      "pattern": "-yyyy-MM-dd",
      "alwaysIncludePattern": true,
      "level": "DEBUG",
      "numBackups": 30
    }
  }]
}
